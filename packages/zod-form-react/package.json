{
  "name": "@zodapp/zod-form-react",
  "version": "0.0.0",
  "private": false,
  "description": "React bindings for zod-form",
  "author": "zodapp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zodapp/zodapp.git",
    "directory": "packages/zod-form-react"
  },
  "homepage": "https://github.com/zodapp/zodapp#readme",
  "bugs": {
    "url": "https://github.com/zodapp/zodapp/issues"
  },
  "keywords": ["zod", "form", "react", "validation"],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./common": "./src/common/index.ts",
    "./media": "./src/media/index.ts",
    "./media/types": "./src/media/types.ts",
    "./mediaResolvers": "./src/mediaResolvers/index.ts",
    "./def": "./src/def/index.ts",
    "./utils/dateConverter": "./src/utils/dateConverter.ts",
    "./utils/externalKey": "./src/utils/externalKey.ts"
  },
  "scripts": {
    "lint": "eslint .",
    "build": "rimraf dist && tsc",
    "check-types": "tsc --noEmit",
    "clean": "rimraf dist .turbo node_modules",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --watch",
    "docs:typedoc": "node ../../tools/gen-typedoc.mjs"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.19.7",
    "@types/react": "^19.2.10",
    "eslint": "^9.34.0",
    "rimraf": "^6.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@zodapp/zod-extendable": "workspace:*",
    "@zodapp/zod-form": "workspace:*",
    "@zodapp/zod-transform": "workspace:*",
    "dayjs": "^1.11.13",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "@tanstack/react-form": "^1.27.7"
  },
  "volta": {
    "extends": "../../package.json"
  }
